C23456
      FUNCTION LINEAR1(P,F0,F1)
      IMPLICIT NONE
      DOUBLE PRECISION LINEAR1,P,F0,F1
      LINEAR1=F0+P*(F1-F0)
      RETURN
      END
C
      FUNCTION LINEAR2(P,Q,F00,F10,F01,F11)
      IMPLICIT NONE
      DOUBLE PRECISION LINEAR2,LINEAR1
      DOUBLE PRECISION P,Q,F00,F10,F01,F11
      DOUBLE PRECISION TMP0,TMP1
      EXTERNAL LINEAR1
      TMP0=LINEAR1(P,F00,F10)
      TMP1=LINEAR1(P,F01,F11)
      LINEAR2=LINEAR1(Q,TMP0,TMP1)
      RETURN
      END
C
      FUNCTION LINEAR3(P,Q,R,F000,F100,F010,F110,F001,F101,F011,F111)
      IMPLICIT NONE
      DOUBLE PRECISION LINEAR3,LINEAR2,LINEAR1
      DOUBLE PRECISION P,Q,R,F000,F100,F010,F110,F001,F101,F011,F111
      DOUBLE PRECISION TMP00,TMP10,TMP01,TMP11
      EXTERNAL LINEAR2,LINEAR1 
      TMP00=LINEAR1(P,F000,F100)
      TMP10=LINEAR1(P,F010,F110)
      TMP01=LINEAR1(P,F001,F101)
      TMP11=LINEAR1(P,F011,F111)
      LINEAR3=LINEAR2(Q,R,TMP00,TMP10,TMP01,TMP11)
      RETURN
      END

      
